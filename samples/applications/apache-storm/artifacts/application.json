{
    "applicationId": "storm-group-app",
    "alias": "storm-group-app",
    "components": {
        "groups": [
            {
                "name": "storm-group",
                "alias": "my-storm-group",
                "deploymentPolicy": "deployment-policy-1",
                "groupMinInstances": 1,
                "groupMaxInstances": 1,
                "cartridges": [
                    {
                        "type": "supervisor",
                        "cartridgeMin": 1,
                        "cartridgeMax": 1,
                        "subscribableInfo": {
                            "alias": "my-supervisor",
                            "autoscalingPolicy": "autoscaling-policy-1",
                            "artifactRepository": {
                                "privateRepo": false,
                                "repoUrl": "https://github.com/imesh/stratos-esb-applications.git",
                                "repoUsername": "",
                                "repoPassword": ""
                            }
                        }
                    },
                    {
                        "type": "nimbus",
                        "cartridgeMin": 1,
                        "cartridgeMax": 1,
                        "subscribableInfo": {
                            "alias": "my-nimbus",
                            "autoscalingPolicy": "autoscaling-policy-1",
                            "artifactRepository": {
                                "privateRepo": false,
                                "repoUrl": "https://github.com/imesh/stratos-php-applications.git",
                                "repoUsername": "",
                                "repoPassword": ""
                            }
                        }
                    },
                    {
                        "type": "zookeeper",
                        "cartridgeMin": 1,
                        "cartridgeMax": 1,
                        "subscribableInfo": {
                            "alias": "my-zookeeper",
                            "autoscalingPolicy": "autoscaling-policy-1",
                            "artifactRepository": {
                                "privateRepo": false,
                                "repoUrl": "https://github.com/imesh/stratos-php-applications.git",
                                "repoUsername": "",
                                "repoPassword": ""
                            }
                        }
                    },
                    {
                        "type": "ui",
                        "cartridgeMin": 1,
                        "cartridgeMax": 1,
                        "subscribableInfo": {
                            "alias": "my-ui",
                            "autoscalingPolicy": "autoscaling-policy-1",
                            "artifactRepository": {
                                "privateRepo": false,
                                "repoUrl": "https://github.com/imesh/stratos-php-applications.git",
                                "repoUsername": "",
                                "repoPassword": ""
                            }
                        }
                    }
                ]
            }
        ]
    }
}

