{
  "type": "wso2esb-481-manager",
  "category": "framework",
  "provider": "wso2",
  "host": "esb.wso2.org",
  "displayName": "WSO2 ESB 4.8.1 Manager",
  "description": "WSO2 ESB 4.8.1 Manager Cartridge",
  "version": "4.8.1",
  "multiTenant": false,
  "loadBalancingIPType": "public",
  "portMapping": [
    {
      "name": "mgt-http",
      "protocol": "http",
      "port": 9763,
      "proxyPort": 0
    },
    {
      "name": "mgt-https",
      "protocol": "https",
      "port": 9443,
      "proxyPort": 0
    },
    {
      "name": "pt-http",
      "protocol": "http",
      "port": 8280,
      "proxyPort": 0
    },
    {
      "name": "pt-https",
      "protocol": "https",
      "port": 8243,
      "proxyPort": 0
    }
  ],
  "iaasProvider": [
    {
      "type": "openstack",
      "imageId": "<REGION>/<BASE_IMAGE_ID>",
      "property": [
        {
          "name": "instanceType",
          "value": "<REGION>/<FLAVOUR_ID>"
        },
        {
          "name": "keyPair",
          "value": "<KEY_PAIR_NAME>"
        },
        {
          "name": "securityGroups",
          "value": "default"
        }
      ],
      "networkInterfaces": [
        {
          "networkUuid": "<NETWORK_UUID>"
        }
      ]
    }
  ],
  "property": [
    {
      "name": "payload_parameter.CONFIG_PARAM_CLUSTERING",
      "value": "true"
    },
    {
      "name": "payload_parameter.LB_IP",
      "value": "172.17.8.102"
    }
  ]
}