{
      "type": "greg",
      "provider": "wso2",
      "host": "greg.wso2.com",
      "displayName": "GREG",
      "description": "GREG Cartridge",
      "version": "4.6.0",
      "multiTenant": "true",
      "portMapping": [
         {
            "protocol": "http",
            "port": "9763",
            "proxyPort": "80"
         },
         {
            "protocol": "https",
            "port": "9443",
            "proxyPort": "443"
         }
       ],
      "deployment": {
        },
       "iaasProvider": [
        {
          "type": "openstack",
          "imageId": "REGION/BASE_IMAGE_ID",
          "maxInstanceLimit": "4",
          "property": [
            {
             "name": "instanceType",
             "value": "REGION/3"
            }
          ]
        }
      ],
      "loadBalancer": {
        "type": "lb",
        "property": {
            "name": "default.load.balancer",
            "value": "true"
        }
      },
      "property": [
        {
          "name": "PRIMARY",
          "value": "@PRIMARY"
        },        
        {
          "name": "payload_parameter.CLUSTERING",
          "value": "@CLUSTERING"
        }
      ]
}
