{   
      "type": "keymanager",
      "provider": "wso2",
      "serviceGroup": "apim",
      "host": "keymanager.am.wso2.com",
      "displayName": "API Manager - Keymanager",
      "description": "API Manager - Keymanager Cartridge",
      "version": "1.6.0",
      "multiTenant": "true",
      "portMapping": [
         {
            "protocol": "http",
            "port": "9763",
            "proxyPort": "80"
         },
         {
            "protocol": "https",
            "port": "9443",
            "proxyPort": "443"
         }
       ],
      "deployment": {
        },
       "iaasProvider": [
        {
          "type": "ec2",
          "imageId": "ap-southeast-1/ami-3c0f5d6e",
          "maxInstanceLimit": "4",
          "property": [
            {
             "name": "instanceType",
             "value": "m1.medium"
            },
            {
             "name": "keyPair",
             "value": "reks-private-paas"
            }
          ]
        }
      ],
      "loadBalancer": {
        "type": "lb",
        "property": {
            "name": "default.load.balancer",
            "value": "true"
        }
      },    
    
 }
